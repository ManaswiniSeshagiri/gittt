client 1

 1  ip a s
    2  hostname
    3  vim /etc/hosts
    4  cat /etc/hosts
    5  vim /etc/ssh/sshd_config
    6  ssh-keygen
    7  ssh-copy-id root@server.example.com
    8  ssh root@server.example.com
    9  mkdir /proo/
   10  cd /proo/
   11  yum install git
   12  git init
   13  vim index.txt
   14  git add index.txt
   15  git commit -m "first" index.txt
   16  git remote add origin root@server.example.com
   17  git remote add origin root@server.example.com:/pro
   18  git remote remove origin
   19  git remote add origin root@server.example.com:/pro
   20  git push origin master
   21  history

